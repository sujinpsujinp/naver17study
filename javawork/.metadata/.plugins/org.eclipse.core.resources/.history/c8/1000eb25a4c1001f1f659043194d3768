package day1224;

import java.io.File;

public class Ex2File {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		File file1=new File("d:/naver1210/Score.txt");
		File file2=new File("d:/naver1210/Score.txt");
		System.out.println(file1.length());//글자길이-바이트단위
		System.out.println(file1.canExecute());//true
		System.out.println(file1.canRead());//읽을 수 있는지
		System.out.println(file1.canWrite());//쓸 수 있는지
		
		System.out.println(file1.exists());//파일이 존재하면 true
		
	}

}
