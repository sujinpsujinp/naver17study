package day1226;

public class Sawon {
	
	private String sawonName;
	private int age;
	private String hp;
	private String address;
	
	public Sawon() {
		
	}

	public Sawon(String sawonName, int age, String hp, String address) {
		super();//상속이 아닌데도 있는건 크게 신경 안써도 됨
		this.sawonName = sawonName;
		this.age = age;
		this.hp = hp;
		this.address = address;
	}
	
	

}
