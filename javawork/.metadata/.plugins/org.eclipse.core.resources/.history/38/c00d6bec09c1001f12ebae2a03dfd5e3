package day1223;

public class P320 {
	
	public class Vehicle{
		//메소드 선언
		public void run() {
			System.out.println("차량이 달립니다.");
		}
	}
	
	public class Bus extends Vehicle
	{
		//메소드 재정의(오버라이딩)
		@Override
		public void run() {
			// TODO Auto-generated method stub
			System.out.println("버스가 달립니다.");
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	
		
	}

}
