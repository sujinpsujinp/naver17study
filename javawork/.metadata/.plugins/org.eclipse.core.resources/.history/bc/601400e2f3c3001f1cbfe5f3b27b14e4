package day1227;

//<T>는 Type 파라미터임을 뜻하는 기호로, 타입이 필요한 자리에 
//T를 사용한다. T는 클래스 타입만 가능하다.
class GenericType<T>
{
	T []v;
	
	public void set(T[] n)
	{
		v=n;
	}
	
	public void print()
	{
		for(T s:v)
		{
			System.out.println(s+" ");
		}
		System.out.println();
	}	
}

public class Ex1Type {
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		
		
	}

}
