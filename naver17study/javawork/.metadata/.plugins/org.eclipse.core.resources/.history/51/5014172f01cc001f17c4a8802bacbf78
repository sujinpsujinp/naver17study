package day0106db;

public class PersonDto {
	private int num;
	private String name;
	private int age;
	private String hp;
	private String blood;
	
	public PersonDto(int num, String name, int age, String hp, String blood) {
		super();
		this.num = num;
		this.name = name;
		this.age = age;
		this.hp = hp;
		this.blood = blood;
	}
	
	

}
